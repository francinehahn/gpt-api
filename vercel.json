{
    "version": 2,
    "builds": [
        {
            "src": "/api/run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/run.py"
        },
        {
            "src": "/favicon.ico",
            "status": 200,
            "headers": {
                "Content-Type": "image/x-icon"
            }
        }
    ]
}