{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "dependencies": {
        "Flask": "1.1.2",
        "Werkzeug": "0.16.1"
    }
}